{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "es2015",
    "stage-0",
    "react"
  ],
  "retainLines": true,
  "plugins": [
    "dynamic-import-node",
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    ["import-inspector", {
      "serverSideRequirePath": true,
      "webpackRequireWeakId": true
    }],
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }],
    "syntax-dynamic-import"
  ]
}
